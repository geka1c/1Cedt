    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втОстатки.ПунктВыдачи,
	СУММА(втОстатки.КоличествоБуфернаяЗона) КАК БуфернаяЗона,
	СУММА(втОстатки.КоличествоОтгрузка) КАК Отгрузка
ПОМЕСТИТЬ втСтатусы
ИЗ
	(ВЫБРАТЬ
		ТранзитОстатки.ПокупкаСсылка КАК Заказ,
		ТранзитОстатки.МестоХранения,
		ТранзитОстатки.Габарит,
		ТранзитОстатки.Участник,
		ТранзитОстатки.Точка КАК ПунктВыдачи,
		ТранзитОстатки.Партия,
		ТранзитОстатки.КоличествоОстаток КАК КоличествоБуфернаяЗона,
		0 КАК КоличествоОтгрузка,
		ТранзитОстатки.МестоХранения.БуфенаяЗона КАК БуфенаяЗона,
		ЛОЖЬ КАК ЗаказУпакован
	ИЗ
		РегистрНакопления.Транзит.Остатки(, МестоХранения.БуфенаяЗона) КАК ТранзитОстатки

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		УпакованныеЗаказыОстатки.Покупка,
		УпакованныеЗаказыОстатки.МестоХранения,
		УпакованныеЗаказыОстатки.Габарит,
		УпакованныеЗаказыОстатки.Участник,
		УпакованныеЗаказыОстатки.Точка,
		УпакованныеЗаказыОстатки.Партия,
		УпакованныеЗаказыОстатки.КоличествоОстаток,
		0,
		УпакованныеЗаказыОстатки.МестоХранения.БуфенаяЗона,
		ИСТИНА
	ИЗ
		РегистрНакопления.УпакованныеЗаказы.Остатки(, МестоХранения.БуфенаяЗона) КАК УпакованныеЗаказыОстатки

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		ТранзитОстатки.ПокупкаСсылка КАК Заказ,
		ТранзитОстатки.МестоХранения,
		ТранзитОстатки.Габарит,
		ТранзитОстатки.Участник,
		ТранзитОстатки.Точка КАК ПунктВыдачи,
		ТранзитОстатки.Партия,
		0,
		ТранзитОстатки.КоличествоОстаток,
		ТранзитОстатки.МестоХранения.БуфенаяЗона,
		ЛОЖЬ КАК ЗаказУпакован
	ИЗ
		РегистрНакопления.Транзит.Остатки(,
		НЕ МестоХранения.БуфенаяЗона) КАК ТранзитОстатки

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		УпакованныеЗаказыОстатки.Покупка,
		УпакованныеЗаказыОстатки.МестоХранения,
		УпакованныеЗаказыОстатки.Габарит,
		УпакованныеЗаказыОстатки.Участник,
		УпакованныеЗаказыОстатки.Точка,
		УпакованныеЗаказыОстатки.Партия,
		0,
		УпакованныеЗаказыОстатки.КоличествоОстаток,
		УпакованныеЗаказыОстатки.МестоХранения.БуфенаяЗона,
		ИСТИНА
	ИЗ
		РегистрНакопления.УпакованныеЗаказы.Остатки(,
		НЕ МестоХранения.БуфенаяЗона) КАК УпакованныеЗаказыОстатки) КАК втОстатки
СГРУППИРОВАТЬ ПО
	втОстатки.ПунктВыдачи
;
////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТочкиРаздачи.Ссылка,
	втСтатусы.БуфернаяЗона,
	втСтатусы.Отгрузка
ИЗ
	Справочник.ТочкиРаздачи КАК ТочкиРаздачи
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ втСтатусы КАК втСтатусы
		ПО втСтатусы.ПунктВыдачи = ТочкиРаздачи.Ссылка</queryText>
      <mainTable>Catalog.ТочкиРаздачи</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
