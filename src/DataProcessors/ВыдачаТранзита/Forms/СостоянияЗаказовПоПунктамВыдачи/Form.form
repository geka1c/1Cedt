  <attributes>
    <name>СостоянияОтгрузки</name>
    <title>
      <key>ru</key>
      <value>Состояния отгрузки</value>
    </title>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
